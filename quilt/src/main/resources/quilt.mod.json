{
  "schema_version": 1,
  "mixins": [
    "eternal_starlight-common.mixins.json",
    "eternal_starlight.mixins.json"
  ],
  "quilt_loader": {
    "group": "cn.leolezury",
    "id": "eternal_starlight",
    "version": "${version}",
    "metadata": {
      "name": "Eternal Starlight",
      "description": "A Minecraft dimension mod with structures for you to explore and bosses for you to defeat!",
      "contributors": {
        "LeoLezury": "Owner"
      },
      "contact": {
        "homepage": "https://github.com/LeoMinecraftModding",
        "issues": "https://github.com/LeoMinecraftModding/Eternal-Starlight/issues",
        "sources": "https://github.com/LeoMinecraftModding/Eternal-Starlight"
      }
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "cn.leolezury.eternalstarlight.quilt.ESQuilt"
      ],
      "client": [
        "cn.leolezury.eternalstarlight.quilt.client.ESQuiltClient"
      ],
      "mm:early_risers": [
        "cn.leolezury.eternalstarlight.quilt.asm.ESASM"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20"
      }
    ]
  }
}